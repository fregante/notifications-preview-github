{
  "name": "Notifications Preview for GitHub",
  "homepage_url": "https://github.com/tanmayrajani/github-notifications-preview",
  "version": "0.4.0",
  "manifest_version": 2,
  "description": "Notifications Preview for GitHub with same page pop-over!",
  "icons": {
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://github.com/*"
    ],
    "css": [
      "style.css"
    ],
    "js": [
      "vendor/jquery.js",
      "index.js"
    ]
  }]
}